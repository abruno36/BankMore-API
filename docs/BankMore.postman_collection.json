								"exec": [
									"if (pm.response.code >= 200 && pm.response.code <= 299) {\r",
									"    var jsonData = pm.response.json();\r",
									"    \r",
									"    // Tente todas as possibilidades\r",
									"    var token = jsonData.token || \r",
									"                jsonData.accessToken || \r",
									"                jsonData.access_token ||\r",
									"                (jsonData.tokens && jsonData.tokens.accessToken);\r",
									"    \r",
									"    if (token) {\r",
									"        pm.environment.set(\"token\", token);\r",
									"        console.log(\"✅ Token salvo:\", token.substring(0, 50) + \"...\");\r",
									"    } else {\r",
									"        console.log(\"❌ Estrutura desconhecida:\", jsonData);\r",
									"    }\r",
									"}"
